l-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"Content\" prop=\"content\">\r\n          <div id=\"editor\"></div>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport E from \"wangeditor\";\r\nimport hljs from \"highlight.js\";\r\n\r\nexport default {\r\n  name: \"NewBlog\",\r\n\r\n  data() {\r\n    return {\r\n      form: {},\r\n      rules: {},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      tagsArr: [],\r\n      categoryList: [],\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.$request.get('/category/selectAll').then(res => {\r\n      this.categoryList = res.data || []\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    save() {   // 保存按钮触发的逻辑  它会触发新增或者更新\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.form.tags = JSON.stringify(this.tagsArr)\r\n          this.form.content = this.editor.txt.html()\r\n          this.$request({\r\n            url: this.form.id ? '/blog/update' : '/blog/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('success')\r\n              this.load(1)\r\n              this.fromVisible = false\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    handleCoverSuccess(res) {\r\n      this.form.cover = res.data\r\n    },\r\n\r\n    setRichText() {\r\n      this.$nextTick(() => {\r\n        this.editor = new E(`#editor`)\r\n        this.editor.highlight = hljs\r\n        this.editor.config.uploadImgServer = this.$baseUrl + '/files/editor/upload'\r\n        this.editor.config.uploadFileName = 'file'\r\n        this.editor.config.uploadImgHeaders = {\r\n          token: this.user.token\r\n        }\r\n        this.editor.config.uploadImgParams = {\r\n          type: 'img',\r\n        }\r\n        this.editor.create()  // 创建\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAwCA,OAAAA,CAAA;AACA,OAAAC,IAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,OAAA;MACAC,YAAA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,QAAA,CAAAC,GAAA,wBAAAC,IAAA,CAAAC,GAAA;MACA,KAAAL,YAAA,GAAAK,GAAA,CAAAd,IAAA;IACA;EACA;EAEAe,OAAA;IACAC,KAAA;MAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAnB,IAAA,CAAAoB,IAAA,GAAAjB,IAAA,CAAAkB,SAAA,MAAAd,OAAA;UACA,KAAAP,IAAA,CAAAsB,OAAA,QAAAC,MAAA,CAAAC,GAAA,CAAAC,IAAA;UACA,KAAAf,QAAA;YACAgB,GAAA,OAAA1B,IAAA,CAAA2B,EAAA;YACAC,MAAA,OAAA5B,IAAA,CAAA2B,EAAA;YACA5B,IAAA,OAAAC;UACA,GAAAY,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAgB,IAAA;cAAA;cACA,KAAAC,QAAA,CAAAC,OAAA;cACA,KAAAC,IAAA;cACA,KAAAC,WAAA;YACA;cACA,KAAAH,QAAA,CAAAI,KAAA,CAAArB,GAAA,CAAAsB,GAAA;YACA;UACA;QACA;MACA;IACA;;IAEAC,mBAAAvB,GAAA;MACA,KAAAb,IAAA,CAAAqC,KAAA,GAAAxB,GAAA,CAAAd,IAAA;IACA;IAEAuC,YAAA;MACA,KAAAC,SAAA;QACA,KAAAhB,MAAA,OAAA3B,CAAA;QACA,KAAA2B,MAAA,CAAAiB,SAAA,GAAA3C,IAAA;QACA,KAAA0B,MAAA,CAAAkB,MAAA,CAAAC,eAAA,QAAAC,QAAA;QACA,KAAApB,MAAA,CAAAkB,MAAA,CAAAG,cAAA;QACA,KAAArB,MAAA,CAAAkB,MAAA,CAAAI,gBAAA;UACAC,KAAA,OAAA5C,IAAA,CAAA4C;QACA;QACA,KAAAvB,MAAA,CAAAkB,MAAA,CAAAM,eAAA;UACAC,IAAA;QACA;QACA,KAAAzB,MAAA,CAAA0B,MAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}